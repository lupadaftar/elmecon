        <service class="Abdroz\FoodMenu\Api\FoodmenuRepositoryInterface" method="getPrice" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/abdroz_foodmenu/getMenuByStatus/:status" method="GET">
        <service class="Abdroz\FoodMenu\Api\FoodmenuRepositoryInterface" method="getStatus" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/abdroz_foodmenu/getMenuByIngredients/:ingredients" method="GET">
        <service class="Abdroz\FoodMenu\Api\FoodmenuRepositoryInterface" method="getIngredients" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
</routes>